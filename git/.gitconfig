# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = Alex Grieco
	email = awgrieco@gmail.com
	signingkey = ~/.ssh/github_ed25519.pub
[url "git@github.com:"]
	insteadOf = https://github.com/
[checkout]
	defaultBare = false
[color "status"]
	branch = normal
	added = 10 normal
	updated = 10 normal
	localbranch = normal
	changed = 11 normal
	header = normal
	remotebranch = normal
	nobranch = normal bold
	unmerged = 9 normal
	untracked = normal dim
[color "branch"]
	plain = normal
	remote = normal
	current = normal reverse
	local = normal
	upstream = normal
[color "remote"]
	hint = normal dim
	success = 2 normal
	error = 1 normal
	warning = 3 normal
[color "interactive"]
	header = normal dim bold
	prompt = 15 4
	help = normal dim
	error = normal bold
[color "diff"]
	commit = normal
	newMoved = 15 3
	oldMovedAlternativeDimmed = 7 5 dim
	frag = normal dim
	oldMovedAlternative = 7 5
	contextBold = normal bold
	newBold = 15 2 bold
	context = normal
	oldMoved = 7 3
	oldBold = 7 1 bold
	newMovedDimmed = 15 3 dim
	func = normal dim
	meta = normal dim
	newDimmed = 15 2 dim
	newMovedAlternative = 15 5
	new = 15 2
	newMovedAlternativeDimmed = 15 5 dim
	whitespace = normal 9
	oldMovedDimmed = 7 3 dim
	oldDimmed = 7 1 dim
	old = 7 1
[color "decorate"]
	branch = normal
	tag = normal
	remotebranch = normal
	stash = normal dim
	grafted = normal bold
	HEAD = normal italic
[diff]
	colorMoved = zebra
[core]
	pager = less --RAW-CONTROL-CHARS
	excludesfile = ~/.gitignore-global
[commit]
	gpgsign = true
[gpg]
	format = ssh
[maintenance]
	repo = /Users/alexgrieco/.utils/zsh-snap
	repo = /Users/alexgrieco/.utils/catppuccin/iterm
	repo = /Users/alexgrieco/.utils/marlonrichert/zcolors
	repo = /Users/alexgrieco/.utils/marlonrichert/zsh-autocomplete
	repo = /Users/alexgrieco/.utils/marlonrichert/zsh-edit
	repo = /Users/alexgrieco/.utils/marlonrichert/zsh-hist
	repo = /Users/alexgrieco/.utils/trapd00r/LS_COLORS
	repo = /Users/alexgrieco/.utils/zsh-users/zsh-autosuggestions
	repo = /Users/alexgrieco/.utils/zsh-users/zsh-syntax-highlighting
